<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductOfferStorage">
        <property name="idProductOffer" type="int"/>
        <property name="productOfferReference" type="string"/>
        <property name="idMerchant" type="int"/>
        <property name="merchantProfile" type="MerchantProfileStorage"/>
    </transfer>

    <transfer name="MerchantProfileStorage">
        <property name="merchantUrl" type="string"/>
    </transfer>

    <transfer name="MerchantProfileStorageUrl">
        <property name="url" type="string"/>
    </transfer>
</transfers>
